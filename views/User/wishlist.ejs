<script>
    function removeProductFromWishlist( productId) {
            fetch('/remove-from-wishlist', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                   
                    productId: productId
                })
            })
                .then(response => response.json())
                .then(result => {
                    console.log(result);

                   

                    window.location.reload(); 
                })
                .catch(error => {
                    console.error('Error:', error);
                });
        }
</script>
