        };
    </script>

    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // Attach the validateForm function to the input events of the relevant form fields
            document.getElementById("register-fullname").addEventListener('input', validateForm);
            document.getElementById("register-email-2").addEventListener('input', validateForm);
            document.getElementById("register-phone").addEventListener('input', validateForm);
            document.getElementById("register-password-2").addEventListener('input', validateForm);
        });

        function validateForm() {
            // Reset previous error messages
            document.getElementById("fullname-error").innerText = "";
            document.getElementById("email-error").innerText = "";
            document.getElementById("phone-error").innerText = "";
            document.getElementById("password-error").innerText = "";

            // Get form values
            var fullname = document.getElementById("register-fullname").value;
            var email = document.getElementById("register-email-2").value;
            var phone = document.getElementById("register-phone").value;
            var password = document.getElementById("register-password-2").value;

            // Validation rules
            var fullnameRegex = /^[a-zA-Z\s]+$/;

            var emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;

            var phoneRegex = /^\d{10}$/;
            
            var passwordRegex = /^(?=.*\d)(?=.*[A-Z]).{8,}$/;

            // Validate Full Name
            if (!fullnameRegex.test(fullname)) {
                document.getElementById("fullname-error").innerText = "Full Name should contain only letters and spaces.";
            }

            // Validate Email
            if (!emailRegex.test(email)) {
                document.getElementById("email-error").innerText = "Invalid email address.";
            }

            // Validate Phone Number
            if (!phoneRegex.test(phone)) {
                document.getElementById("phone-error").innerText = "Phone Number should have 10 digits.";
            }

            // Validate Password
            if (!passwordRegex.test(password)) {
                document.getElementById("password-error").innerText = "Password should be at least 8 characters, including numbers and an uppercase letter.";
            }
        }
    </script>

