        <form action="/admin/users/edit?id=<%= userDetails._id %>" method="post">
            <section class="content-main">
                <div class="row">
                    <div class="col-9">
                        <div class="content-header">
                            <h2 class="content-title">Edit User</h2>
                            <div>
                                <button class="btn btn-md rounded font-sm hover-up">Publish</button>
                            </div>
                        </div>
                    </div>
                    <div class="card mb-4">
        
                        <div class="card-body">
                            <div class="col-md-4 mb-2">
                                <label class="form-label" for="name">Name</label>
                                <input value="<%=userDetails.name%>" type="text" id="name" name="name" placeholder="Type here" class="form-control"
                                    required />
                            </div>
                            <div class="col-md-4 mb-2">
                                <label class="form-label" for="email">Email</label>
                                <input value="<%=userDetails.email%>" type="email" id="email" name="email" placeholder="Type here" class="form-control"
                                    required />
                            </div>
                            <div class="col-md-4 mb-2">
                                <label class="form-label" for="mobile">Mobile Number</label>
                                <input value="<%=userDetails.phone%>" type="tel" id="mobile" name="mobile" placeholder="Type here" class="form-control"
                                    required />
                            </div>
                            <div class="col-md-4 mb-2" hidden>
                                <label class="form-label" for="password">Password</label>
                                <input value="<%=userDetails.password%>" type="password" id="password" name="password" placeholder="Type here"
                                    class="form-control" required />
                            </div>
                            <div class="col-md-7 mb-2">
                                <label class="form-label" for="address">Detailed Address</label>
                                <textarea value="<%=userDetails.address%>" id="address" name="address" placeholder="Type here" class="form-control"
                                    rows="4"></textarea>
                            </div>
                            <div class="mb-3">
                                <label for="verificationStatus">Verification Status:</label>
                                <div class="form-check">
                                    <input type="radio" id="verified" name="verified" class="form-check-input" value="1"
                                        <%=userDetails.is_verified==="1" ? 'checked' : '' %>>
                                    <label for="verified" class="form-check-label">Verified</label>
                                </div>
                                <div class="form-check">
                                    <input type="radio" id="notVerified" name="verified" class="form-check-input" value="0"
                                        <%=userDetails.is_verified==="0" ? 'checked' : '' %>>
                                    <label for="notVerified" class="form-check-label">Not Verified</label>
                                </div>
                            </div>

                            <div class="col-md-4 mb-2">
                                <label class="form-label" for="status">Status</label>
                                <div class="form-check">
                                    <input type="radio" id="active" name="status" class="form-check-input" value="1" <%=userDetails.is_active==="1"
                                        ? 'checked' : '' %>>
                                    <label for="active" class="form-check-label">Active</label>
                                </div>
                                <div class="form-check">
                                    <input type="radio" id="blocked" name="status" class="form-check-input" value="0" <%=userDetails.is_active==="0"
                                        ? 'checked' : '' %>>
                                    <label for="blocked" class="form-check-label">Blocked</label>
                                </div>
                            </div>

        
                        </div>
        
        
                    </div>
                </div>
            </section>
        </form>

