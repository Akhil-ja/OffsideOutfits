                <div class="card-body">
                    <div class="table-responsive">
                        <table id="ordersTable" class="table align-middle table-nowrap mb-0">
                            <thead class="table-light">
                                <tr>
                                    <th scope="col" class="text-center">
                                        <div class="form-check align-middle"></div>
                                    </th>
                                    <th class="align-middle" scope="col">Order ID</th>
                                    <th class="align-middle" scope="col">User Name</th>
                                    <th class="align-middle" scope="col">Date</th>
                                    <th class="align-middle" scope="col">Total</th>
                                    <th class="align-middle" scope="col">Coupon Applied</th>
                                    <th class="align-middle" scope="col">Payment Method</th>
                                    <th class="align-middle" scope="col">Discount</th>
                                </tr>
                            </thead>
                            <tbody>
                        <% const reversedOrders=orders.slice().reverse(); %>
                            <% reversedOrders.forEach(order=> { %>
                                    <tr>
                                        <td class="text-center">
                                            <div class="form-check"></div>
                                        </td>
                                        <td><a href="#" class="fw-bold" style="font-size: smaller;">#<%= order.orderID %></a></td>
                                        <td>
                                            <%= order.user.name %>
                                        </td>
                                        <td>
                                            <%= order.orderDate.toLocaleDateString("en-US", { year: 'numeric' , month: 'short' ,
                                                day: 'numeric' }) %>
                                        </td>
                                        <td>Rs: <%= order.orderTotal.toFixed(1) %>
                                        </td>
                                        <td>
                                            <% if (order.couponApplied) { %>
                                                <span class="badge badge-pill badge-soft-success">
                                                    <%= order.couponApplied.code %>
                                                </span>
                                                <% } else { %>
                                                    <span class="badge badge-pill badge-soft-danger">Not Applied</span>
                                                    <% } %>
                                        </td>
                                        <td>
                                            <% if (order.PaymentMethod==='COD' ) { %>
                                                <i class="material-icons md-money font-xxl text-muted mr-5"></i>COD
                                                <% } else if (order.PaymentMethod==='RazorPay' ) { %>
                                                    <i class="material-icons md-payment font-xxl text-muted mr-5"></i>RazorPay
                                                    <% } %>
                                        </td>
                                        <td>
                                            <% if (order.discountPercentage) { %>
                                                <%= order.discountPercentage %>%
                                                    <% } else { %>
                                                        <span>No discount</span>
                                                        <% } %>
                                        </td>
                                    </tr>
                                    <% }); %>
                            </tbody>
                        </table>
                    </div>

                    <div class="pagination-area mt-15 mb-50">
                        <nav aria-label="Page navigation example">
                            <ul class="pagination justify-content-start">
                                <% if (currentPage> 1) { %>
                                    <li class="page-item">
                                        <a class="page-link" href="?page=<%= currentPage - 1 %>" aria-label="Previous">
                                            <i class="material-icons md-chevron_left"></i>
                                        </a>
                                    </li>
                                    <% } %>
                    
                                        <% const visiblePages=5; // Number of visible page links %>
                                            <% const startPage=Math.max(1, currentPage - Math.floor(visiblePages / 2)); %>
                                                <% const endPage=Math.min(totalPages, startPage + visiblePages - 1); %>
                    
                                                    <% for (let i=startPage; i <=endPage; i++) { %>
                                                        <li class="page-item <%= i === currentPage ? 'active' : '' %>">
                                                            <a class="page-link" href="?page=<%= i %>">
                                                                <%= i %>
                                                            </a>
                                                        </li>
                                                        <% } %>
                    
                                                            <% if (currentPage < totalPages) { %>
                                                                <li class="page-item">
                                                                    <a class="page-link" href="?page=<%= currentPage + 1 %>"
                                                                        aria-label="Next">
                                                                        <i class="material-icons md-chevron_right"></i>
                                                                    </a>
                                                                </li>
                                                                <% } %>
                            </ul>
                        </nav>
                    </div>


