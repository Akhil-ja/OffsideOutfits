<script>
    document.addEventListener('DOMContentLoaded', function () {
            const chartCanvas = document.getElementById('chartCanvas');
            let myChart = null;

            function decodeHtmlEntities(text) {
                var textArea = document.createElement('textarea');
                textArea.innerHTML = text;
                return textArea.value;
            }

        function renderChart(chartData) {
            if (myChart) {
                myChart.destroy();
            }

            const ctx = chartCanvas.getContext('2d');
            myChart = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: chartData.labels,
                    datasets: [
                        {
                            label: 'Revenue',
                            data: chartData.data,
                            backgroundColor: 'rgba(54, 162, 235, 0.7)',
                            borderColor: 'rgba(54, 162, 235, 1)',
                            borderWidth: 1,
                        },
                    ],
                },
                options: {
                    responsive: true, // Make the chart responsive
                    maintainAspectRatio: false, // Disable aspect ratio to prevent distortion
                    scales: {
                        y: {
                            beginAtZero: true,
                        },
                    },
                },
            });
        }

            document.getElementById('btnMonthly').addEventListener('click', function () {
                const monthlyDataString = '<%= JSON.stringify(monthly) %>';
                const monthlyData = JSON.parse(decodeHtmlEntities(monthlyDataString));
                renderChart(monthlyData);
            });

            document.getElementById('btnYearly').addEventListener('click', function () {
                const yearlyDataString = '<%= JSON.stringify(yearly) %>';
                const yearlyData = JSON.parse(decodeHtmlEntities(yearlyDataString));
                renderChart(yearlyData);
            });

            document.getElementById('btnDaily').addEventListener('click', function () {
                const dailyDataString = '<%= JSON.stringify(daily) %>';
                const dailyData = JSON.parse(decodeHtmlEntities(dailyDataString));
                renderChart(dailyData);
            });

            document.getElementById('btnWeekly').addEventListener('click', function () {
                const weeklyDataString = '<%= JSON.stringify(weekly) %>';
                const weeklyData = JSON.parse(decodeHtmlEntities(weeklyDataString));
                renderChart(weeklyData);
            });

        
        const orderChartData = JSON.parse(decodeHtmlEntities('<%= JSON.stringify(orderChart) %>'));
        const orderChartCanvas = document.getElementById('chartCanvas2');
        const orderChartCtx = orderChartCanvas.getContext('2d');

        new Chart(orderChartCtx, {
            type: 'pie',
            data: {
                labels: orderChartData.statuses,
                datasets: [{
                    data: orderChartData.counts,
                    backgroundColor: [
                        'rgba(255, 99, 132, 0.7)',
                        'rgba(54, 162, 235, 0.7)',
                        'rgba(75, 192, 192, 0.7)',
                        'rgba(255, 205, 86, 0.7)',
                        'rgba(153, 102, 255, 0.7)',
                        'rgba(255, 159, 64, 0.7)'
                    ],
                    borderColor: [
                        'rgba(255, 99, 132, 1)',
                        'rgba(54, 162, 235, 1)',
                        'rgba(75, 192, 192, 1)',
                        'rgba(255, 205, 86, 1)',
                        'rgba(153, 102, 255, 1)',
                        'rgba(255, 159, 64, 1)'
                    ],
                    borderWidth: 1
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
            }
        });
    });
</script>


