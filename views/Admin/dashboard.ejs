  <script>
    window.onload = function () {
        document.getElementById('filterForm').addEventListener('submit', function (event) {
            event.preventDefault();

            const selectedDate = document.getElementById('filterByDate').value;
            fetch(`/admin/dashboard?selectedDate=${selectedDate}`, { method: 'POST' })
                .then(response => response.json())
                .then(data => {
                    renderTable(data.orders);
                })
                .catch(error => console.error('Error:', error));
        });

        function renderTable(orders) {
            const tableBody = document.querySelector('#ordersTable tbody');

            tableBody.innerHTML = '';

            if (orders.length === 0) {
                const noOrdersRow = document.createElement('tr');
                noOrdersRow.innerHTML = `<td colspan="8" class="text-center">No orders for the selected date</td>`;
                tableBody.appendChild(noOrdersRow);
            } else {
                orders.forEach(order => {
                    const row = document.createElement('tr');
                    row.innerHTML = `
                        <td class="text-center">
                            <div class="form-check"></div>
                        </td>
                        <td><a href="#" class="fw-bold" style="font-size: smaller;">#${order._id}</a></td>
                        <td>${order.user.name}</td>
                        <td>${new Date(order.orderDate).toLocaleDateString("en-US", { year: 'numeric', month: 'short', day: 'numeric' })}</td>
                        <td>Rs: ${order.orderTotal.toFixed(1)}</td>
                        <td>${order.couponApplied ? `<span class="badge badge-pill badge-soft-success">${order.couponApplied.code}</span>` : '<span class="badge badge-pill badge-soft-danger">Not Applied</span>'}</td>
                        <td>${order.PaymentMethod === 'COD' ? '<i class="material-icons md-money font-xxl text-muted mr-5"></i>COD' : '<i class="material-icons md-payment font-xxl text-muted mr-5"></i>RazorPay'}</td>
                        <td>${order.discountPercentage ? `${order.discountPercentage}%` : '<span>No discount</span>'}</td>`;
                    tableBody.appendChild(row);
                });
            }
        }
    };
</script>


<script>
    function pdf() {
        const table = document.getElementById("ordersTable");
        html2pdf().from(table).save();
    }
</script>


