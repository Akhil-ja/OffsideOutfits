            <div class="card-body">
<article class="itemlist">
    <% users.forEach(user=> { %>
        <div class="row align-items-center">
            <div class="col col-check flex-grow-0">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox">
                </div>
            </div>
            <div class="col-lg-2 col-sm-2 col-4 col-price">
                <span>Email</span>
            </div>
            <div class="col-lg-4 col-sm-4 col-8 flex-grow-1 col-name">
                <a class="itemside" href="#">
                    <div class="info">
                        <h6 class="mb-0">
                            <%= user.name %>
                        </h6>
                        <span class="text-secondary">
                            <%= user.email %>
                        </span>
                    </div>
                </a>
            </div>
            <div class="col-lg-2 col-sm-2 col-4 col-status">
                <% if (user.is_active==="1" ) { %>
                    <span class="badge rounded-pill alert-success">Active</span>
                    <% } else { %>
                        <span class="badge rounded-pill alert-danger">Blocked</span>
                        <% } %>
            </div>
            <div class="col-lg-1 col-sm-2 col-4 col-date"hidden>
                <span>Current Time</span>
            </div>
            <div class="col-lg-2 col-sm-2 col-4 col-action text-end">
                
                  <a href="/admin/users/edit?id=<%= user._id %>" class="btn btn-sm font-sm rounded btn-brand m-2">
    <i class="material-icons md-edit"></i> Edit
</a>

                    <% if (user.is_verified !=="1" ) { %>
                    <a href="/admin/users/delete?id=<%= user._id %>" class="btn btn-sm font-sm btn-light rounded delete-btn">
                        <i class="material-icons md-delete_forever"></i> Delete
                    </a>
                    <% } %>
            </div>
        </div> <!-- row .// -->
        <% }); %>
</article> <!-- itemlist  .// -->

