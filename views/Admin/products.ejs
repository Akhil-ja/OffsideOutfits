<div class="card-body">
    <ul class="list-group">
        <% for (let i=0; i < products.length; i++) { %>
            <li class="list-group-item d-flex justify-content-between align-items-center">
                <div class="img-wrap me-3">
                    <img src="/productAssets/<%= products[i].images[0] %>" alt="Product Image" class="img-fluid rounded"
                        style="max-height: 100px; max-width: 100px;">
                </div>
                <div class="info-wrap flex-fill">
                    <a href="#" class="title text-truncate mb-0">
                        <%= products[i].pname %>
                    </a>
                    <div class="price mt-2">
                       
                                <%= products[i].price %>
                           
                    </div>
                </div>
                <div class="action-buttons">
                    <a href="/admin/products/edit-product?id=<%= products[i]._id %>"
                        class="btn btn-sm btn-primary me-2">
                        <i class="material-icons md-edit"></i> Edit
                    </a>
                    <% if (products[i].is_listed !=="1" ) { %>
                        <a href="/admin/delete-product/<%= products[i]._id %>" class="btn btn-sm btn-danger">
                            <i class="material-icons md-delete_forever"></i> Delete
                        </a>
                        <% } %>
                </div>
            </li>
            <% } %>
    </ul>
